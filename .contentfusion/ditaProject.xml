<?xml version="1.0" encoding="UTF-8" standalone="yes"?>
<project xmlns="https://www.dita-ot.org/project">
    <context id="project" name="Project">
        <input href="../project.ditamap"/>
    </context>
    <publication id="webhelp" transtype="webhelp-responsive"/>
    <publication id="pdf" transtype="pdf-css-html5"/>
    <deliverable id="wh-responsive" name="Project to WebHelp Responsive">
        <context idref="project"/>
        <output href="webhelp-output"/>
        <publication idref="webhelp">
            <param href="../publishing/oxygen/oxygen-tiles.opt" name="webhelp.publishing.template"/>
            <param name="editlink.enable.edit.in.content.fusion.workspace" value="false"/>
        </publication>
    </deliverable>
    <deliverable id="pdf" name="Project to PDF">
        <context idref="project"/>
        <output href="pdf-output"/>
        <publication idref="pdf">
            <param href="cf://templates/Oxygen" name="pdf.publishing.template"/>
        </publication>
    </deliverable>
</project>
